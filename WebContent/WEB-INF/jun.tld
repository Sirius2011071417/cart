        <attribute>
        	<name>date</name>
        	<required>true</required>
        	<rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
    <tag>
        <description>RefererTag</description>
        <name>referer</name>
        <tag-class>cn.web.tag.RefererTag</tag-class>
        <body-content>empty</body-content>
        <attribute>
        	<name>site</name>
        	<required>true</required>
        	<rtexprvalue>true</rtexprvalue>
        </attribute>
        
        <attribute>
        	<name>page</name>
        	<required>true</required>
        	<rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
    <tag>
        <description>IfTag</description>
        <name>if</name>
        <tag-class>cn.web.tag.IfTag</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
