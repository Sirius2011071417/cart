        <tag-class>cn.web.tag.TagDemo3</tag-class>
        <body-content>JSP</body-content>
    </tag>
    
    <tag>
        <description>Change content</description>
        <name>demo4</name>
        <tag-class>cn.web.tag.TagDemo4</tag-class>
        <body-content>JSP</body-content>
    </tag>
    
    <tag>
        <description>SimpleTagDemo1</description>
        <name>sdemo1</name>
        <tag-class>cn.web.tag.SimpleTagDemo1</tag-class>
        <body-content>scriptless</body-content>
    </tag>
    
    <tag>
        <description>SimpleTagDemo2</description>
