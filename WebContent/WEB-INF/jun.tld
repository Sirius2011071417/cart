        <tag-class>cn.web.tag.SimpleTagDemo2</tag-class>
        <body-content>empty</body-content>
    </tag>
    
    <tag>
        <description>SimpleTagDemo3</description>
        <name>sdemo3</name>
        <tag-class>cn.web.tag.SimpleTagDemo3</tag-class>
        <body-content>scriptless</body-content>
    </tag>
    
    <tag>
        <description>SimpleTagDemo4</description>
        <name>sdemo4</name>
        <tag-class>cn.web.tag.SimpleTagDemo4</tag-class>
        <body-content>scriptless</body-content>
    </tag>
    
    <tag>
        <description>SimpleTagAttributeDemo1</description>
        <name>sademo1</name>
        <tag-class>cn.web.tag.SimpleTagAttributeDemo1</tag-class>
        <body-content>scriptless</body-content>
        <attribute>
        	<name>count</name>
        	<required>true</required>
        	<rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    
    <tag>
        <description>SimpleTagAttributeDemo2</description>
        <name>sademo2</name>
        <tag-class>cn.web.tag.SimpleTagAttributeDemo2</tag-class>
